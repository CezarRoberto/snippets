// Place to save some prefix that i use on my vsCode
// contact me for any question: https://www.linkedin.com/in/rodrigues-cezar/
// Example:
{
	"console.log": {
		"scope": "javascript,typescript",
		"prefix": "cl",
		"body": [
			"console.log('Ello');",
			"$2"
		],
		"description": "Log output to console"
	},
	"tests.repository": {
		"scope": "javascript,typescript",
		"prefix": "tr",
		"body": [
			"describe('$0', () => {",
			"  let sut: $1",
			"",
			"  ${4|beforeEach, beforeAll|}(() => {",
      			"  ",
      		"})",
				"  describe('$2', () => {",
					"    it('should $3', () => {",
					"",
					"    });",
				"  });",
			"});",
		],
		"description": "Prefix for testing the class repository calling a class"
	},
	"tests.any": {
		"scope": "javascript,typescript",
		"prefix": "tc",
		"body": [
			"describe('$0', () => {",
			"  let sut: $1",
			"",
			"  ${4|beforeEach, beforeAll|}(() => {",
      			"  ",
      		"})",
					"  it('should $3', () => {",
					"",
					"  });",
			"});",
		],
		"description": "Prefix for testing the class repository calling a class"
	}
}
